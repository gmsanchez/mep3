            <Action velocity="90" ID="DynamixelCommandAction" timeout="5" tolerance="2" result="0" server_name="dynamixel_command/arm_left_motor_mid" position="0"/>
            <Action ID="ScoreboardTaskAction" points="1" task="store_green_sample_to_camp"/>
        </Sequence>
    </BehaviorTree>
