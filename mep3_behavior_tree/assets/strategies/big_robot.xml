    <BehaviorTree ID="task_move_green_sample">
        <Sequence>
            <Action behavior_tree="" ID="PreciseNavigateToAction" goal="0.8;0.23;180"/>
           <Parallel failure_threshold="1" success_threshold="3">
                <Action velocity="90" ID="DynamixelCommandAction" timeout="5" tolerance="2" result="0" server_name="dynamixel_command/arm_right_motor_base" position="-90"/>
                <Action velocity="90" ID="DynamixelCommandAction" timeout="5" tolerance="2" result="0" server_name="dynamixel_command/arm_right_motor_mid" position="100"/>
                <Action velocity="90" ID="DynamixelCommandAction" timeout="5" tolerance="2" result="0" server_name="dynamixel_command/arm_right_motor_gripper" position="80"/>
            </Parallel>
            <Action ID="VacuumPumpCommandAction" result="1" connect="1" server_name="vacuum_pump_command/arm_right_connector"/>
            <Action velocity="90" ID="DynamixelCommandAction" timeout="5" tolerance="2" result="0" server_name="dynamixel_command/arm_right_motor_mid" position="0"/>
           <Parallel failure_threshold="1" success_threshold="2">
                <SubTree ID="skill_swap" __shared_blackboard="true"/>
                <Action behavior_tree="backward" ID="PreciseNavigateToAction" goal="1.13;0.138;0"/>
            </Parallel>
            <Action velocity="90" ID="DynamixelCommandAction" timeout="5" tolerance="2" result="0" server_name="dynamixel_command/arm_left_motor_gripper" position="90"/>
