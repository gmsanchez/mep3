<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Control ID="TaskSequenceControl">
            <SubTree ID="task_excavation_squares" __shared_blackboard="true"/>
