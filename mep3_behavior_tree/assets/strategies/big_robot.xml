                <Action velocity="90" ID="DynamixelCommandAction" timeout="5" tolerance="8" result="0" server_name="dynamixel_command/arm_left_motor_mid" position="05"/>
                <Action velocity="90" ID="DynamixelCommandAction" timeout="5" tolerance="2" result="0" server_name="dynamixel_command/arm_left_motor_gripper" position="60"/>
