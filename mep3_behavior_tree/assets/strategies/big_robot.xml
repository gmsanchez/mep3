    <BehaviorTree ID="skill_retract_hands">
        <Parallel failure_threshold="1" success_threshold="1">
            <Action velocity="90" ID="DynamixelCommandAction" timeout="5" tolerance="2" result="0" server_name="dynamixel_command/arm_right_motor_base" position="0"/>
