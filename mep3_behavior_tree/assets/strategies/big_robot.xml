    <BehaviorTree ID="task_replica_and_statuette">
        <Sequence>
            <SubTree ID="skill_retract_hands" __shared_blackboard="true"/>
            <Action behavior_tree="" ID="PreciseNavigateToAction" goal="0.6;-0.7;0"/>
            <Action behavior_tree="" ID="PreciseNavigateToAction" goal="1;-0.45;60"/>
            <Action behavior_tree="backward" ID="PreciseNavigateToAction" goal="1.157;-0.675;135"/>
            <Action velocity="90" ID="DynamixelCommandAction" timeout="2" tolerance="2" result="0" server_name="dynamixel_command/hand_mid_L" position="90"/>
            <Action ID="ScoreboardTaskAction" points="5" task="remove_statuette_from_pedestal"/>
            <Action ID="ScoreboardTaskAction" points="10" task="install_replica_on_pedestal"/>
            <Action acceleration_angular="0" ID="MotionCommandAction" acceleration_linear="0" command="forward" velocity_linear="0" result="success" velocity_angular="0" value="0.3"/>
            <Action behavior_tree="" ID="NavigateToAction" goal="1.25;0.7;-90"/>
            <Action behavior_tree="backward" ID="PreciseNavigateToAction" goal="1.25;0.9;-90"/>
            <Action velocity="90" ID="DynamixelCommandAction" timeout="2" tolerance="2" result="0" server_name="dynamixel_command/hand_mid_L" position="-15"/>
            <Action ID="ScoreboardTaskAction" points="15" task="install_statuette_on_display_cabinet"/>
            <Action acceleration_angular="0" ID="MotionCommandAction" acceleration_linear="0" command="forward" velocity_linear="0" result="success" velocity_angular="0" value="0.7"/>
        </Sequence>
    </BehaviorTree>
